{
  "name": "dp-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write '**/src/**/*.{ts,tsx,js}'",
    "lint": "eslint '**/src/**/*.{ts,tsx,js}'",
    "lint:fix": "eslint --fix '**/src/**/*.{ts,tsx,js}'",
    "test": "jest"
  },
  "dependencies": {
    "@theme-ui/presets": "^0.3.0",
    "body-parser": "^1.19.0",
    "dotenv-load": "^2.0.0",
    "next": "^9.5.0",
    "next-connect": "^0.8.1",
    "next-env": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "**/src/**/*.{ts,tsx,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  }
}
